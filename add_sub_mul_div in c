#include<stdio.h>
#include<conio.h>
void main()
{
  int a,b,ans;
  printf("enter the number");
  scanf("%d%d",&a,&b);
  ans=a+b;
  printf("%d+%d=%d\n",a,b,ans);
  ans=a-b;
  printf("%d-%d=%d\n",a,b,ans);
  ans=a*b;
  printf("%d*%d=%d\n",a,b,ans);
  ans=a/b;
  printf("%d/%d=%d",a,b,ans);
  getch();
}
